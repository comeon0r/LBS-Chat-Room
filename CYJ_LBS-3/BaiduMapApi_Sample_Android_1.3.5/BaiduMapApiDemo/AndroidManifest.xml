        <activity android:name=".tab3_chattingWithFriends" > </activity>
        <activity android:name=".chattingGroupUI" > </activity>
        <activity android:name=".chattingGroupAdapter" > </activity>
        <activity android:name=".button_takePicture" android:screenOrientation="sensor" 
